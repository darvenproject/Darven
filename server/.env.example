# Database Configuration
DATABASE_URL=postgresql://username:password@your-rds-endpoint.region.rds.amazonaws.com:5432/darven

# AWS S3 Configuration
AWS_REGION=us-east-1
S3_BUCKET_NAME=darven-uploads
# AWS credentials (optional if using IAM roles)
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key

# JWT Configuration
JWT_SECRET_KEY=your-super-secret-jwt-key-change-this-in-production
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS Configuration (if needed)
ALLOWED_ORIGINS=https://shopdarven.netlify.app,http://localhost:3000,http://localhost:3001
